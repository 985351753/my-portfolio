{
  "stylelint.enable": true,
  "stylelint.validate": [
    "css",
    "scss"
  ],
  "stylelint.configFile": ".stylelintrc.cjs",
  "stylelint.trace.server": "verbose",
  "css.validate": false,
  "scss.validate": false,
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "liveServer.settings.root": "/dist/htdocs",
}